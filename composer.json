{
  "name": "shopping/apitk-manipulation-bundle",
  "version": "0.2.2",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "",
  "authors": [
    {
      "name": "Alexander Dormann",
      "email": "alexdo@alexdo.de"
    }
  ],
  "autoload": {
    "psr-4": { "Shopping\\ApiTKManipulationBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
